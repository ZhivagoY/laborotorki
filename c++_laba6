#include <iostream>
int main() {
	int matrix[100][100];
	int n, m;
	int proizved = 1;
	int proizved2;
	int max = -99999999;
	int k;
	std::cin >> n >> m;
	if (n > 100 || m > 100) {
		std::cout << "Dimension error!" << std::endl;
		return -1;
	}

	for (int i = 0; i < n; i++)
		for (int j = 0; j < m; j++)
			std::cin >> matrix[i][j];

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			proizved *= matrix[i][j];
		}
		if (proizved > max) {
			max = proizved;
			proizved2 = proizved;
			proizved = 1;
			k = i;
			}
	}

	for (int j = 0; j < m; j++)
		matrix[k][j] = proizved2;

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++)
			std::cout << matrix[i][j] << " ";
		std::cout << std::endl;
	}
		
	return 0;
}
