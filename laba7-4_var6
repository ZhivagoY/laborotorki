#include <iostream>
int main() {
	setlocale(LC_ALL, "Russian");
	int n;
	std::cout << "Введите размер последовательности: " << std::endl;
	std::cin >> n;
	if (n > 10000) {
		std::cout << "Слишком много";
	}
	else {
	    std::cout << "Введите элементы: " << std::endl;
	    for (int i = 0; i < n; i++) {
		    int x;
		    std::cin >> x;
	    }
	}

	return 0;
}
